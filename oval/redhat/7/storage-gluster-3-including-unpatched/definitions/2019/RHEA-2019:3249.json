  "Metadata": {
    "Title": "RHEA-2019:3249: glusterfs bug fix and enhancement update (Low)",
    "AffectedList": [
      {
        "Family": "unix",
        "Platforms": [
          "Native Client for RHEL 7 for Red Hat Storage"
        ]
      }
    ],
    "References": [
