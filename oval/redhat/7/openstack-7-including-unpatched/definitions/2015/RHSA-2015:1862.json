  "Metadata": {
    "Title": "RHSA-2015:1862: Red Hat Enterprise Linux OpenStack Platform 7 director update (Moderate)",
    "AffectedList": [
      {
        "Family": "unix",
        "Platforms": [
          "Red Hat Enterprise Linux OpenStack Platform director 7.0 for RHEL 7"
        ]
      }
    ],
    "References": [
